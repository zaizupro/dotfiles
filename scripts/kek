#!/bin/sh

 if [ -f ~/.bash_zaz ]; then
    source ~/.bash_zaz
 fi



 __printor "┌────────────────────────────────────┐" $YELLOW
 __printor "[CLEANING STARTED] at [`__currentTTY`]" $YELLOW
 __printor "[`date -R`]" $YELLOW
 __printor "└────────────────────────────────────┘" $YELLOW


make clean


__printor "┌────────────────────────────────────┐" $GREEN
__printor "[CLEANING DONE] at [`__currentTTY`]" $GREEN
__printor "[`date -R`]" $GREEN
__printor "└────────────────────────────────────┘" $GREEN

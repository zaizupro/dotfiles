#!/bin/bash
#toilet -f future -t "START `date +%T`"

if [ -f ~/.bash_zaz ]; then
   source ~/.bash_zaz
fi



__printfulline
__printor "[`__currentTTY`]"
__printor "[`date -R`]"
__printfulline

MKSTARTDATE=$(date +%s)
makek.sh
#toilet -f future -t "STOP  `date +%T`"
MKENDDATE=$(date +%s)
BTMSG="BUILDED IN $((${MKENDDATE} - ${MKSTARTDATE})) SECONDS!"


__printfulline
__printor "[${BTMSG}]"
__printor "[`date -R`]"
__printfulline

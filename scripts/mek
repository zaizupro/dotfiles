#!/bin/bash

if [ -f ~/.bash_zaz ]; then
   source ~/.bash_zaz
fi


declare -a TMPMSG
TMPMSG=("BUILDING STARTED AT $(__currentTTY)" "$(date -R)")
__blockprintor TMPMSG[@] ${YELLOWFGBG}


MKSTARTDATE=$(date +%s)
makek.sh
#|  sed "s/^/[$YELLOW$(date +%T)$NC]/"
RESULT=$?
MKENDDATE=$(date +%s)

if [ $RESULT -eq 0 ]; then
    MKRESULT="BUILDING COMPLETED"
    COLOR=("${GREENFGBG}")
else
    MKRESULT="BUILDING FAILED"
    COLOR=("${REDFGBG}")
fi


TMPMSG=("${MKRESULT} IN $((${MKENDDATE} - ${MKSTARTDATE})) SECONDS!!!" "$(date -R)")
__blockprintor TMPMSG[@] $(echo ${COLOR})

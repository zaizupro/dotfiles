[alias]
    last = log -1 HEAD
    lasd = diff HEAD^ HEAD
      st = status
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
       l = log --oneline --graph --decorate
      co = checkout
    repo = config --get remote.origin.url
      br = branch -a


[push]
    default = simple

[color]
    ui = true

[color "decorate"]
    branch = 16 208
    remoteBranch = 220 94
    tag = 16 44
    stash = 33
    HEAD = 196 52

[color "diff"]
    plain = 221
    meta = 222 130
    frag = 16 221
    func = 222 94
    old = 196 52
    new = 154 22
    whitespace = 52 208

[color "branch"]
    plain = 221
    upstream = 154
    current = 222 130
      local = 222
     remote = 208

[color "status"]
    added = 208 22
    header = 222
    changed = 220 94
    updated = 154 22
    untracked = 196 52
    branch = 16 208

[include]
    path = .gitconfig.user
